{
  "name": "react-ipython-notebook",
  "version": "0.1.0",
  "description": "A React component for read-only iPython Notebooks",
  "main": "dist/index.js",
  "scripts": {
    "lint": "standard src/*.js src/**/*.js test/*.js test/**/*.js",
    "test": "npm run lint && mocha --compilers js:babel/register test/*-spec.js test/**/*-spec.js",
    "watch-test": "mocha -wG --compilers js:babel/register test/*-spec.js test/**/*-spec.js",
    "watch-lint": "nodemon --exec \"npm run lint\"",
    "notify-lint": "./scripts/notify-lint.sh",
    "watch-notify-lint": "nodemon --exec \"npm run notify-lint\"",
    "bundle": "browserify -t [ babelify --presets [ react es2015 ] ] src/index.js -o dist/bundle.js",
    "uglify": "uglifyjs  --compress --mangle -- dist/bundle.js > dist/bundle.min.js",
    "start": "./node_modules/.bin/webpack-dev-server --config example/webpack.config.js --content-base example/",
    "build-example": "./node_modules/.bin/webpack --config example/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plotly/react-ipython-notebook"
  },
  "keywords": [
    "python",
    "ipython",
    "notebook",
    "ipynb",
    "react"
  ],
  "author": "Ricky Reusser",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plotly/react-ipython-notebook/issues"
  },
  "homepage": "https://github.com/plotly/react-ipython-notebook#readme",
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^6.1.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "html-webpack-plugin": "^1.6.2",
    "mdast": "^2.1.0",
    "mdast-react": "^0.3.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.2",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "standard": "^5.3.1",
    "uglify-js": "^2.6.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "react-remarkable": "^1.1.1",
    "remarkable": "^1.6.0"
  }
}
